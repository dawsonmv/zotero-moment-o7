{
  "manifest_version": 2,
  "name": "Zotero Moment-o7",
  "version": "2.0.0",
  "description": "Archive web resources to prevent link rot. Automatically saves pages to Internet Archive when adding items to Zotero.",
  "author": "Harding Center for Risk Literacy",
  "homepage_url": "https://github.com/dawsonmv/zotero-moment-o7",
  
  "applications": {
    "zotero": {
      "id": "zotero-memento@tran.org",
      "strict_min_version": "6.999",
      "strict_max_version": "7.0.*",
      "update_url": "https://raw.githubusercontent.com/dawsonmv/zotero-moment-o7/master/update.json"
    }
  },
  
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png"
  },
  
  "bootstrap": "bootstrap.js"
}