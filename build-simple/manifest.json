{
  "manifest_version": 2,
  "name": "Zotero Moment-o7 (KISS)",
  "version": "1.0.0",
  "description": "Simple web archiving for Zotero. Archives pages to Internet Archive and Archive.today.",
  "author": "Simplified by KISS principles",
  "homepage_url": "https://github.com/dawsonmv/zotero-moment-o7",
  
  "applications": {
    "zotero": {
      "id": "zotero-moment-o7-kiss@github.com",
      "strict_min_version": "6.999",
      "strict_max_version": "7.*"
    }
  },
  
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png"
  },
  
  "preferences": {
    "url": "chrome://zotero-moment-o7/content/preferences-simple.xhtml"
  },
  
  "bootstrap": "bootstrap-simple.js"
}